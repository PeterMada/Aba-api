{
  "kind": "collectionType",
  "collectionName": "news_tags",
  "info": {
    "singularName": "news-tag",
    "pluralName": "news-tags",
    "displayName": "News-tag",
    "name": "news-tag"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "workshops": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::workshop.workshop",
      "mappedBy": "tags"
    },
    "news": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::new.new",
      "inversedBy": "tags"
    }
  }
}
