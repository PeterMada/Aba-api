    },
    "social_media_sites": {
      "collection": "social-media-site"
    },
    "TitleBefore": {
      "type": "string"
    },
    "TitleAfter": {
      "type": "string"
    },
    "Email": {
      "type": "string"
    },
    "MetaKeywords": {
